#include<stdio.h>
int main()
{
    int op,x,y,res=0;
    scanf("%d",&op);
    scanf("%d %d",&x,&y);
    if(op==1){
        res=x+y;
        printf("%d",res);
    }
    else if(op==2){
        res=x-y;
        printf("%d",res);
    }
    else if(op==3){
        res=x*y;
        printf("%d",res);
    }
    else{
        printf("invalid");
    }
}
